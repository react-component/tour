{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"],
      "@rc-component/tour": ["src/Tour.tsx"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
